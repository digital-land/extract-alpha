---
  pagination:
    data: geoReferenceResults
    size: 1
    alias: geoReferenceResult
  permalink: "/prototypes/geo-reference/results/{{ geoReferenceResult.filename | slugify }}/"
---
{% extends "layouts/prototype-default.njk" %}
{% set pageName = "Location found" %}

{% from "govuk/components/button/macro.njk" import govukButton %}

{% from "components/title/macro.html" import title %}
{% from "prototypes/geo-reference/macros/resultPage.njk" import resultPage %}




{% block content %}
  {{super()}}

  {{title(pageName, geoReferenceResult.filename)}}


    <hr class="govuk-section-break govuk-section-break--m govuk-section-break--visible govuk-!-margin-bottom-7"/>
  

    {{resultPage(geoReferenceResult)}}


     <hr class="govuk-section-break govuk-section-break--m govuk-section-break--visible govuk-!-margin-bottom-7"/>
  

    {{ govukButton({text: "Upload a new document",href: "/prototypes/geo-reference"}) }}

{% endblock %}

 {% block bodyEnd %}
  {{ super() }}
  <script>
    document.querySelectorAll("[data-function='copyable']").forEach((element) => {
      element.addEventListener("click", (event) => {
        navigator.clipboard.writeText(event
          .target
          .dataset
          .clipboardText);
      });
    });
  </script>
{% endblock %}